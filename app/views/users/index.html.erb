<% provide(:title, 'Cidadãos') %>

<div class="row">
  <div class="span12">

    <h1>Todos os cidadãos</h1>

    <ul class="users">
      <% @users.each do |user| %>
        <li>
          <%= gravatar_for user, size: 50 %>
          <%= link_to user.name, user %>
        </li>
      <% end %>
    </ul>

  </div>
</div>