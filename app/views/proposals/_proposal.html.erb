<div class="row-fluid">
  <div class="span12" style="background-color: #EEEEEE">
    <div class="span3 text-center" style="background-color: #DDDDDD">
      <h3><%= proposal.score %></h3>
      <p><%= render 'shared/vote_buttons', :proposal => proposal %></p>
    </div>
    <div class="span9">
      <h4><%= link_to proposal.title, proposal %></h4>
      <small class="muted">Proposto por <%= render 'shared/link_to_user', :user => proposal.user %> em <%= proposal.created_at.to_time.strftime('%e/%m/%Y') %></small>
    </div>
  </div>
</div>
<br>
